<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.key_project.sed.core.annotationTypes" name="Annotation Types" schema="schema/org.key_project.sed.core.annotationTypes.exsd"/>
      <extension
            point="org.eclipse.core.runtime.preferences">
         <initializer
               class="org.key_project.sed.core.util.SEDPreferenceUtilInitializer">
         </initializer>
      </extension>
      <extension
            point="org.eclipse.core.expressions.propertyTesters">
         <propertyTester
               class="org.key_project.sed.core.propertyTester.StackFrameHasVariablesPropertyTester"
               id="org.key_project.sed.core.stackFrameHasVariablesPropertyTester"
               namespace="org.key_project.sed.core"
               properties="stackFrameHasVariables"
               type="org.eclipse.debug.core.model.IStackFrame">
         </propertyTester>
         <propertyTester
               class="org.key_project.sed.core.propertyTester.HasSEDebugTargetPropertyTester"
               id="org.key_project.sed.core.hasSEDDebugTargetPropertyTester"
               namespace="org.key_project.sed.core"
               properties="hasSEDDebugTarget"
               type="org.eclipse.debug.core.ILaunch">
         </propertyTester>
         <propertyTester
               class="org.key_project.sed.core.propertyTester.HasSESlicerPropertyTester"
               id="org.key_project.sed.core.hasSEDSlicerPropertyTester"
               namespace="org.key_project.sed.core"
               properties="hasSEDSlicerPropertyTester"
               type="org.key_project.sed.core.model.ISEVariable">
         </propertyTester>
      </extension>
      <extension
            point="org.key_project.sed.core.annotationTypes">
         <annotationType
               class="org.key_project.sed.core.annotation.impl.SearchAnnotationType">
         </annotationType>
         <annotationType
               class="org.key_project.sed.core.annotation.impl.CommentAnnotationType">
         </annotationType>
         <annotationType
               class="org.key_project.sed.core.annotation.impl.BreakpointAnnotationType">
         </annotationType>
         <annotationType
               class="org.key_project.sed.core.annotation.impl.SliceAnnotationType">
         </annotationType>
      </extension>
</plugin>
