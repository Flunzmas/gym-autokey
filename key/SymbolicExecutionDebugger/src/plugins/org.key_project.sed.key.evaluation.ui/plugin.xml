<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            name="SED Evaluation"
            id="org.key_project.sed.key.evaluation.commands.category">
      </category>
      <command
            name="Understanding Proof Attempts"
            categoryId="org.key_project.sed.key.evaluation.commands.category"
            id="org.key_project.sed.key.evaluation.commands.debuggingProofsCommand">
      </command>
      <command
            categoryId="org.key_project.sed.key.evaluation.commands.category"
            id="org.key_project.sed.key.evaluation.commands.reviewingCodeCommand"
            name="Reviewing Code">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.commandImages">
      <image
            commandId="org.key_project.sed.key.evaluation.commands.debuggingProofsCommand"
            icon="icons/evaluation_KeY.png">
      </image>
      <image
            commandId="org.key_project.sed.key.evaluation.commands.reviewingCodeCommand"
            icon="icons/evaluation_Java.png">
      </image>
   </extension>   
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="org.key_project.sed.key.evaluation.commands.debuggingProofsCommand"
            class="org.key_project.sed.key.evaluation.handlers.UnderstandingProofAttemptsHandler">
      </handler>
      <handler
            class="org.key_project.sed.key.evaluation.handlers.ReviewingCodeHandler"
            commandId="org.key_project.sed.key.evaluation.commands.reviewingCodeCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.key_project.sed.key.evaluation.commands.debuggingProofsCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+M2+8"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
      <key
            commandId="org.key_project.sed.key.evaluation.commands.reviewingCodeCommand"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+9">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?before=help">
         <menu
               label="Evaluation"
               mnemonic="v"
               id="org.key_project.sed.key.evaluation.menus.evaluationMenu">
            <command
                  commandId="org.key_project.sed.key.evaluation.commands.debuggingProofsCommand"
                  mnemonic="U"
                  id="org.key_project.sed.key.evaluation.menus.sampleCommand">
            </command>
            <command
                  commandId="org.key_project.sed.key.evaluation.commands.reviewingCodeCommand"
                  id="org.key_project.sed.key.evaluation.menus.sampleCommand"
                  mnemonic="R">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.key_project.sed.key.ui.preference.page.key_symbolic_debug"
            class="org.key_project.sed.key.evaluation.preference.page.SEDEvaluationServerPreferencePage"
            id="org.key_project.sed.key.evaluation.preference.page.SEDEvaluationServerPreferencePage"
            name="Evaluation">
      </page>
   </extension>
</plugin>
